  <%= form_for :sessions, url: login_path do |f| %>
  <%= label_tag :username %>
  <br>
  <%= f.text_field :username, placeholder: "Username", autofocus: true %>
  <br>
  <%= label_tag :password %>
  <br>
  <%= f.password_field  :password, placeholder: "Password" %>
  <br>
  <%= f.submit "Log In" %>
<% end %>