<li class='track_row bottom'>
  <div class='left'>
    <%= form_tag({ :action => :create,:controller => :tracks}, :id =>"song_upload", :multipart => true) do %>
      <%= file_field_tag :file %>
      <%= submit_tag("Upload") %>
    <% end %>
  </div>
  <div id="dropzone" class="center">
    <h2><a href="/" class='add_track'>DROP A BEAT HERE</a></h2>
  </div>
  <div class='right'>
    <%= form_for( :version,
                  :html => { :method => "post" },
                  :url => update_version_path(@repository, @version)) do |f|  %>
      <%= f.submit "SAVE IT" %>
    <% end %>
  </div>
</li>
